-- Table: public."producto"

-- DROP TABLE "producto";

create sequence nextval('seq_id_producto');

CREATE TABLE "producto"
(
    "id" numeric(7) NOT NULL DEFAULT nextval('seq_id_producto'),
    "nombre" character varying(4000),
    "precio" numeric(8),
    CONSTRAINT "Producto_pkey" PRIMARY KEY ("id")
)
TABLESPACE pg_default;

ALTER TABLE "producto"
    OWNER to sistemas;


-- Table: public."inventario"

-- DROP TABLE "inventario";

CREATE TABLE "inventario"
(
    "fecha" timestamp without time zone NOT NULL,
    "id_producto" numeric(7) NOT NULL,
    "cantidad_inicial" numeric(9),
    "cantidad_ingresada" numeric(9),
    "cantidad_actual" numeric(9),
    "cantidad_vendido" numeric(9),
    "venta_total" numeric(12),
    CONSTRAINT "Inventario_pkey" PRIMARY KEY ("fecha", "id_producto"),
    CONSTRAINT "Fk_Inventario" FOREIGN KEY ("id_producto")
        REFERENCES "producto" ("id") MATCH SIMPLE
        ON UPDATE CASCADE
        ON DELETE CASCADE
)
TABLESPACE pg_default;

ALTER TABLE "inventario"
    OWNER to sistemas;

-- Table: public."reporte_diario"

-- DROP TABLE public."reporte_diario";

CREATE TABLE "reporte_diario"
(
    "fecha" timestamp without time zone NOT NULL,
    "gasto_dia" numeric(9),
    "gasto_totales" numeric(9),
    "total_vendido" numeric(9),
    "total_actual" numeric(9),
    CONSTRAINT "Reporte_Diario_pkey" PRIMARY KEY ("fecha")
)
TABLESPACE pg_default;

ALTER TABLE "reporte_diario"
    OWNER to sistemas;


-- Table: public."gasto_diario"

-- DROP TABLE public."gasto_diario";

CREATE TABLE "gasto_diario"
(
    "fecha" time without time zone NOT NULL,
    "descripcion" character varying(4000),
    "valor_invertido" numeric(12),
    CONSTRAINT "Gasto_Diario_pkey" PRIMARY KEY ("fecha")
)
TABLESPACE pg_default;

ALTER TABLE "gasto_diario"
    OWNER to sistemas;
